{
  "name": "stlmpp",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-utils": "ng build @stlmpp/utils",
    "build-store": "ng build @stlmpp/store",
    "build-router": "ng build @stlmpp/router",
    "build-control": "ng build @stlmpp/control",
    "build-utils:prod": "ng build @stlmpp/utils --prod",
    "build-store:prod": "ng build @stlmpp/store --prod",
    "build-router:prod": "ng build @stlmpp/router --prod",
    "build-control:prod": "ng build @stlmpp/control --prod",
    "build-all": "yarn build-utils && yarn build-store && yarn build-router && yarn build-control",
    "build-all:prod": "yarn build-utils:prod && yarn build-store:prod && yarn build-router:prod && yarn build-control:prod",
    "coverage-store": "ng test @stlmpp/store --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-utils": "ng test @stlmpp/utils --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-router": "ng test @stlmpp/router --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-control": "ng test @stlmpp/control --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "test-store": "ng test @stlmpp/store --no-watch --code-coverage",
    "test-utils": "ng test @stlmpp/utils --no-watch --code-coverage",
    "test-router": "ng test @stlmpp/router --no-watch --code-coverage",
    "test-control": "ng test @stlmpp/control --no-watch --code-coverage",
    "test-all": "yarn test-store && yarn test-utils && yarn test-router && yarn test-control",
    "combine-coverage": "node combine-coverage.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "rxjs": "~6.6.3",
    "tslib": "~2.0.0",
    "type-fest": "~0.20.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-eslint/builder": "~0.8.0-beta.2",
    "@angular-eslint/eslint-plugin": "~0.8.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "~0.8.0-beta.2",
    "@angular-eslint/schematics": "~0.8.0-beta.2",
    "@angular-eslint/template-parser": "~0.8.0-beta.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.19.1",
    "@typescript-eslint/eslint-plugin": "~4.8.2",
    "@typescript-eslint/parser": "~4.8.2",
    "coveralls": "~3.1.0",
    "eslint": "~7.14.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsdoc": "~30.7.6",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "glob": "~7.1.6",
    "istanbul-api": "~3.0.0",
    "istanbul-lib-coverage": "~3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "karma-jasmine-seed-reporter": "~0.2.0",
    "ng-packagr": "~11.0.2",
    "prettier": "~2.2.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.3"
  }
}
