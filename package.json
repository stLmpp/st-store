{
  "name": "stlmpp",
  "version": "~0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-utils": "ng build @stlmpp/utils",
    "build-store": "ng build @stlmpp/store",
    "build-router": "ng build @stlmpp/router",
    "build-control": "ng build @stlmpp/control",
    "build-utils:prod": "ng build @stlmpp/utils --configuration production",
    "build-store:prod": "ng build @stlmpp/store --configuration production",
    "build-router:prod": "ng build @stlmpp/router --configuration production",
    "build-control:prod": "ng build @stlmpp/control --configuration production",
    "build-all": "yarn build-utils && yarn build-store && yarn build-router && yarn build-control",
    "build-all:prod": "yarn build-utils:prod && yarn build-store:prod && yarn build-router:prod && yarn build-control:prod",
    "coverage-store": "ng test @stlmpp/store --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-utils": "ng test @stlmpp/utils --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-router": "ng test @stlmpp/router --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "coverage-control": "ng test @stlmpp/control --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "test-store": "ng test @stlmpp/store --no-watch --code-coverage",
    "test-utils": "ng test @stlmpp/utils --no-watch --code-coverage",
    "test-router": "ng test @stlmpp/router --no-watch --code-coverage",
    "test-control": "ng test @stlmpp/control --no-watch --code-coverage",
    "test-all": "yarn test-store && yarn test-utils && yarn test-router && yarn test-control",
    "combine-coverage": "node combine-coverage.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "docs-utils": "typedoc projects/stlmpp/utils/src/public-api.ts --out projects/stlmpp/utils/docs --tsconfig projects/stlmpp/utils/tsconfig.lib.json",
    "docs-store": "typedoc projects/stlmpp/store/src/public-api.ts --out projects/stlmpp/store/docs --tsconfig projects/stlmpp/store/tsconfig.lib.json",
    "docs-router": "typedoc projects/stlmpp/router/src/public-api.ts --out projects/stlmpp/router/docs --tsconfig projects/stlmpp/router/tsconfig.lib.json",
    "docs-control": "typedoc projects/stlmpp/control/src/public-api.ts --out projects/stlmpp/control/docs --tsconfig projects/stlmpp/control/tsconfig.lib.json",
    "docs-all": "yarn docs-utils && yarn docs-store && yarn docs-router && yarn docs-control",
    "publish-all": "ts-node ./scripts/publish.ts",
    "version-all": "ts-node ./scripts/version.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "rxjs": "~7.2.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular-eslint/builder": "~12.3.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/schematics": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/jasmine": "~3.8.1",
    "@types/node": "~12.19.16",
    "@types/yargs": "~17.0.2",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "coveralls": "~3.1.1",
    "eslint": "~7.30.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsdoc": "~35.4.3",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "glob": "~7.1.7",
    "istanbul-api": "~3.0.0",
    "istanbul-lib-coverage": "~3.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-jasmine-seed-reporter": "~0.2.0",
    "ng-packagr": "~12.1.2",
    "ora": "~5.4.1",
    "prettier": "~2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.1.0",
    "typescript": "~4.3.5",
    "yargs": "~17.0.1"
  }
}
